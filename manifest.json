{
  "manifest_version": 2,

  "name": "Kill-Switch",
  "description": "Kills any unproductive chrome tabs.",
  "version": "0.1",

  "browser_action": {
    "default_icon": "images/icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs",
    "activeTab",
    "https://ajax.googleapis.com/"
  ]
}